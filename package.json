{
  "name": "ym2151-typescript-cli-zig-cc",
  "version": "1.0.0",
  "description": "YM2151 (Nuked-OPM) 440Hz tone generator using TypeScript and zig cc",
  "main": "dist/phase2/index.js",
  "scripts": {
    "build": "npm run build:native && npm run build:ts",
    "build:native": "node-gyp rebuild",
    "build:ts": "tsc",
    "start": "npm run build && node dist/phase2/index.js",
    "generate": "node dist/phase2/index.js",
    "postinstall": "node-gyp rebuild"
  },
  "keywords": [
    "ym2151",
    "nuked-opm",
    "fm-synthesis",
    "audio",
    "wav"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-addon-api": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "node-gyp": "^11.5.0",
    "typescript": "^5.9.3"
  }
}
